        <main class="page-content">
            <div class="container">
                <div class="container mt-5">
                    <h1 style="color: bisque;">
                        <%=quizName%>
                    </h1>
        
                    <div class="row">
                        <div class="col-sm-8">
                            <div class="card">
                                <div class="card-body">
        
        
                                    <form action="/addQues" method="POST">
                                        <div class="form-group">
                                            <label for="question" style="color: bisque; font-size: medium;">Enter The Question</label>
                                            <input type="text" class="form-control" name="question">

                                        </div>
        
                                        <div class="form-group">
                                            <label for="op1" style="color: bisque; font-size: medium; ">Option 1</label>
                                            <input type="text" class="form-control" style="margin-left: 100px;" name="op1">
                                        </div>
        
                                        <div class="form-group">
                                            <label style="color: bisque; font-size: medium;" for="op2">Option 2</label>
                                            <input type="text" class="form-control" style="margin-left: 90px;" name="op2">
                                        </div>
        
                                        <div class="form-group">
                                            <label for="op3" style="color: bisque; font-size: medium;">Option 3</label>
                                            <input type="text" class="form-control" style="margin-left: 90px;" name="op3">
                                        </div>
        
                                        <div class="form-group">
                                            <label for="op4" style="color: bisque;" font-size: medium;>Option 4</label>
                                            <input type="text" class="form-control" style="margin-left: 100px;" name="op4">
                                        </div>
        
                                        <div class="form-group">
                                            <label for="answer" style="color: bisque; font-size: medium;">Correct Option</label>
                                            <input type="text" class="form-control" style="margin-left: 40px;" name="answer">
                                        </div>
        
                                        <button type="submit" style="color: bisque; font-size: medium;" class="btn btn-dark">Add Question</button>
        
                                    </form>
                                    <a class="btn btn-light btn-lg" href="/testCreated" role="button" style="color: bisque; font-size: medium;">Create Test</a>
                                </div>
                            </div>
                        </div>
        
        
        
                    </div>
                </div>
        
            </div>
        
        </main>
        <!-- page-content" -->
    </div>
    <!-- page-wrapper -->
    <script type='text/javascript' src='https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js'></script>

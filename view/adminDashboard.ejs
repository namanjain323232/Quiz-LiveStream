    <body>
        <%- include('partials/header') %>
        <div class="page-wrapper chiller-theme toggled">
            <a id="show-sidebar" class="btn btn-sm btn-dark" href="#">
                <i class="fas fa-bars" style="color: bisque;"></i>
            </a>
            <%- include('partials/sidebarAdmin') %>
            <!-- sidebar-wrapper  -->
            <main class="page-content">
                <div class="container">
                    <div class="row">
                        <div class="form-group col-md-12">
                            <div class="alert alert-warning" role="alert">
                                <h4 class="alert-heading" style="font-size: medium;">Live Stream</h4>
                                
                                <a href="/Newlivestream" style="color: bisque; font-size: medium;" target="-blank" class="btn btn-sm btn-success mr-2">

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <link type='text/css' rel='stylesheet' href='/css/styleDashboard.css'/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/c3/0.4.10/c3.min.css">  
  <script src="http://d3js.org/d3.v3.min.js" charset="utf-8"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/c3/0.4.10/c3.min.js"></script>
  <title>Document</title>
</head>
<body>
  <%- include('partials/header') %>
  <div class="page-wrapper chiller-theme toggled">
    <a id="show-sidebar" class="btn btn-sm btn-dark" href="#">
      <i class="fas fa-bars"></i>
    </a>
    <%- include('partials/sidebarUser') %>
    <!-- sidebar-wrapper  -->
    <main class="page-content">
      <div class="container">
        <div class="row">
          <div class="real-time-printing"></div>
      <table class="rwd-table">
        <tr>
          <th>S.No</th>
          <th>Quiz</th>
          <th>Review</th>
        </tr>
        <tr> 
          <td data-th="S.No">1</td>
          <td data-th="Quiz">A</td>
          <td data-th="Review">100</td>
        </tr>
        <tr>
          <td data-th="S.No">2</td>
          <td data-th="Quiz">B</td>
          <td data-th="Review">100</td>
        </tr>
        <tr>
          <td data-th="S.No">3</td>
          <td data-th="Quiz">C</td>
          <td data-th="Review">100</td>
        </tr>
        <tr>
          <td data-th="S.No">4</td>
          <td data-th="Quiz">D</td>
          <td data-th="Review">75</td>
        </tr>
        <tr>
          <td data-th="S.No">5</td>
          <td data-th="Quiz">E</td>
          <td data-th="Review">75</td>
        </tr>
        <tr>
          <td data-th="S.No">6</td>
          <td data-th="Quiz">F</td>
          <td data-th="Review">60</td>
        </tr>
      </table>
    </div>

    <h2>Recent Quizzes</h2>

      <div class="rectangles">
        <div class="rectangle" style="background-color: #DA9347">
          <span class="rectangleHeading">My Courses</span>
          <span class="rectangleText">0</span>
        </div>
        <div class="rectangle" style="background-color: #66B92E">
            <span class="rectangleHeading">Total Quizzes</span>
            <span class="rectangleText">0</span>
        </div>
        <div class="rectangle" style="background-color: #2298F1">
            <span class="rectangleHeading">Live Sessions Attended</span>
            <span class="rectangleText">0</span>
        </div>
        <div class="rectangle" style="background-color: #D65B4A">
            <span class="rectangleHeading">Last Purchase</span>
            <span class="rectangleText">0</span>
        </div>
      </div>
  
      </div>
  
    </main>
    <!-- page-content" -->
  </div>
  <!-- page-wrapper -->
  <script type='text/javascript' src='https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js'></script>
  <script type='text/javascript' src='/js/dashboard.js'></script>
</body>
</html>